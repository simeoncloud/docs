{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "SimeonSyncOrganizationSettings",
    "properties": {
        "name": {
            "type": "string",
            "description": "The name of the setting"
        },
        "displayName": {
            "type": "string",
            "description": "The display name of the setting"
        },
        "description": {
            "type": "string",
            "description": "A description of the setting"
        },
        "defaultTextIfEmpty": {
            "type": "string",
            "description": "The default text if the setting is empty"
        },
        "category": {
            "type": "string",
            "description": "The category of the setting"
        },
        "regexValidation": {
            "type": "string",
            "description": "Whether regex validation is applied to the setting"
        },
        "isAdvanced": {
            "type": "boolean",
            "description": "Whether the setting is considered advanced"
        },
        "isSecret": {
            "type": "boolean",
            "description": "Whether the setting is considered secret"
        },
        "isUpdatable": {
            "type": "boolean",
            "description": "Whether the setting is updatable"
        }
    },
    "required": [
        "name",
        "displayName",
        "category"
    ]
}